<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
    <!-- As general rule, every properties in this case-->
		<add key="verbs" value="mp:s"/>
		<add key="boot.module.loader" value="" /> 
		<add key="boot.module.loader.class" value=""/>
		<!-- Se questa chiave vale true , allora viene usato un container osgi per la creazione dei servizi remoti
		     altrimenti usa l'implementazione interna, per il momento la chiave Osgi non ha effetto in quanto
    -->
		<add key="boot.service.container.useOsgi" value="false"/>
		<!-- 
		  recupera il percorso del file xml per la configurazione 
		  di una instanza dell'application server.
		  Nella sottodirectory schema deve 
		  essere presente l'xschema 
      NOTA: e' possibile modificare 
		--> 
		<add key="configurationDir" value="..\..\..\"/>
    <add key="serverConfSchema" value="monoboss-as-config_1_5.xsd"/>
	</appSettings>
</configuration>